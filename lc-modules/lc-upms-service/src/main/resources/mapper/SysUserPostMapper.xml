<mapper namespace="com.zhkj.lc.admin.mapper.SysUserPostMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zhkj.lc.admin.model.entity.SysUserPost">
        <id column="user_id" property="userId" />
        <result column="post_id" property="postId" />
        <result column="tenant_id" property="tenantId" />
